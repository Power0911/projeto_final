<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Escolher">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="241dp"
        android:layout_height="59dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="60dp"
        android:text="Historicos"
        android:textSize="50sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Registos"
        android:layout_width="120dp"
        android:layout_height="57dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#03A9F4"
        android:text="Registos"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_logins" />

    <Button
        android:id="@+id/btn_logins"
        android:layout_width="117dp"
        android:layout_height="54dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="468dp"
        android:backgroundTint="#03A9F4"
        android:text="Login"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_imagem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/images2" />
</androidx.constraintlayout.widget.ConstraintLayout>