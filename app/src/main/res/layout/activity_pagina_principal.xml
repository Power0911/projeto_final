<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FCFDFD"
    tools:context=".Pagina_principal">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="255dp"
        android:layout_height="54dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="48dp"
        android:text="Swap Vehiche"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/cbox_Marcas"
        android:layout_width="291dp"
        android:layout_height="54dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="46dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Spinner
        android:id="@+id/cbox_Modelos"
        android:layout_width="291dp"
        android:layout_height="54dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="132dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Spinner
        android:id="@+id/cbox_Ano"
        android:layout_width="291dp"
        android:layout_height="54dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="224dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Spinner
        android:id="@+id/cbox_Swap"
        android:layout_width="291dp"
        android:layout_height="54dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="316dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/btn_Calcular"
        android:layout_width="124dp"
        android:layout_height="46dp"
        android:layout_marginStart="149dp"
        android:layout_marginTop="41dp"
        android:backgroundTint="#03A9F4"
        app:cornerRadius="20dp"
        android:elevation="2dp"
        android:text="Calcular"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cbox_Swap" />

    <Button
        android:id="@+id/btn_Historico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#03A9F4"
        app:cornerRadius="20dp"
        android:elevation="2dp"
        android:text="Historicos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Calcular" />

    <Button
        android:id="@+id/btn_Voltar"
        android:layout_width="128dp"
        android:layout_height="46dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="76dp"
        android:backgroundTint="#03A9F4"
        app:cornerRadius="20dp"
        android:elevation="2dp"
        android:text="Voltar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Calcular" />
</androidx.constraintlayout.widget.ConstraintLayout>